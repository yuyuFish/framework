	<profiles>
	    <profile>
	        <id>jdk-1.6</id>
	        <activation>
	            <activeByDefault>true</activeByDefault>
	            <jdk>1.6</jdk>
	        </activation>
	    </profile>
	</profiles>
	
	<modules>
		<module>commons-db</module>
		<module>webapp</module>
    	<module>patchca</module>
