		<apache.log4j.version>1.2.17</apache.log4j.version>
		<slf4j.version>1.7.13</slf4j.version>
		<alibaba.fastjson.version>1.2.7</alibaba.fastjson.version>
		<google.gson.version>2.5</google.gson.version>
		<mybatis.version>3.3.0</mybatis.version>
		<mybatis.spring.version>1.2.3</mybatis.spring.version>
		<mysql.jdbc.version>5.1.38</mysql.jdbc.version>
		<spring.version>4.2.4.RELEASE</spring.version>
		<activiti.version>5.19.0.1</activiti.version>
